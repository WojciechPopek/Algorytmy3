def bubble_sort(array):

n = len(array)

swapped = True

while swapped:

swapped = False

for i in range(n - 1):

if array[i] > array[i + 1]:

array[i], array[i + 1] = array[i + 1], array[i]

swapped = True

n -= 1

my_array = [5, 2, 9, 1, 7, 4]

bubble_sort(my_array)

print("Posortowana tablica:", my_array)
