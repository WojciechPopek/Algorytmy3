def insertion_sort(array):
 
for i in range(1, len(array)):
     
value = array[i]
      
j = i - 1

while j >= 0 and array[j] > value:
           
array[j + 1] = array[j]
         
j -= j

array[j + 1] = value

my_array = [5, 2, 9, 1, 7, 4]

insertion_sort(my_array)

print("Posortowana tablica:", my_array)
